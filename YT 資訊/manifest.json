{
  "manifest_version": 3,
  "name": "YouTube Video Details Fetcher",
  "version": "1.0",
  "description": "Fetch video details from YouTube videos.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"

  }

}
